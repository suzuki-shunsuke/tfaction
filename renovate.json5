{
  extends: [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":autodetectPinVersions",
    "group:monorepos",
    "group:recommended",
    "workarounds:all",
    "helpers:pinGitHubActionDigests",
    "github>suzuki-shunsuke/renovate-config",
    "github>aquaproj/aqua-renovate-config#2.0.0",
