  prHourlyLimit: 0,
  prConcurrentLimit: 0,
  branchConcurrentLimit: 0,
  packageRules: [
    {
      // Until https://github.com/suzuki-shunsuke/tfaction/pull/1479 is merged
      matchManagers: ["npm"],
      enabled: false,
    },
  ],
}
